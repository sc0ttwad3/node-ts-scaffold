{
  "defaultSeverity": "warning",
  "rules": {
    "arrow-parens": false,
    "space-before-function-paren": false,
    "only-arrow-functions": [false],
    "no-console": [false],
    "class-name": true,
    "no-var-keyword": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "no-internal-module": true,
    "no-null-keyword": true,
    "prefer-const": true,
    "jsdoc-format": true
  }
}
